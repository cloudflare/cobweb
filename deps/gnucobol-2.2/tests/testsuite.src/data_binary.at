### GnuCOBOL Test Suite

# 2-4-8 big-endian

AT_SETUP([BINARY: 2-4-8 big-endian])
AT_KEYWORDS([binary])

AT_DATA([dump.c], [
#include <stdio.h>
#define COB_LIB_EXPIMP
#include <libcob.h>
COB_EXPIMP int dump (unsigned char *);
int dump (unsigned char *data)
{
  int i;
  for (i = 0; i < 8; i++)
    printf ("%02x", data[[i]]);
  puts ("");
  return 0;
}
])

sed -e 's/@USAGE@/BINARY/' "${TEMPLATE}/numeric-dump.cob" > prog.cob

AT_CHECK([$COMPILE_MODULE dump.c], [0], [], [])
AT_CHECK([$COMPILE -fbinary-size=2-4-8 \
                   -fbinary-byteorder=big-endian prog.cob], [0], [], [])
AT_CHECK([$COBCRUN_DIRECT ./prog], [0],
